{
	"name": "ng",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"prettier:fix": "npx prettier . --write --config .prettierrc --cache --ignore-path .prettierignore --cache-location .prettiercache",
		"eslint:fix": "cross-env TIMING=5 npx eslint . --fix --config .eslintrc --ignore-path .eslintignore --cache --cache-location .eslintcache",
		"stylelint:fix": "npx stylelint \"**/*.scss\" --config .stylelintrc --ignore-path .stylelintignore --cache --cache-location .stylelintcache",
		"lint-staged": "npx lint-staged --config .lintstagedrc",
		"commitlint": "commitlint --edit"
	},
	"dependencies": {
		"@angular/animations": "^16.2.0",
		"@angular/common": "^16.2.0",
		"@angular/compiler": "^16.2.0",
		"@angular/core": "^16.2.0",
		"@angular/forms": "^16.2.0",
		"@angular/platform-browser": "^16.2.0",
		"@angular/platform-browser-dynamic": "^16.2.0",
		"@angular/router": "^16.2.0",
		"normalize.css": "^8.0.1",
		"primeng": "^16.3.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.2.2",
		"@angular-eslint/builder": "16.2.0",
		"@angular-eslint/eslint-plugin": "16.2.0",
		"@angular-eslint/eslint-plugin-template": "16.2.0",
		"@angular-eslint/schematics": "16.2.0",
		"@angular-eslint/template-parser": "16.2.0",
		"@angular/cli": "~16.2.2",
		"@angular/compiler-cli": "^16.2.0",
		"@types/jasmine": "~4.3.0",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.50.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-plugin-canonical": "^4.12.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsdoc": "^46.8.2",
		"eslint-plugin-unicorn": "^48.0.1",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"postcss": "^8.4.30",
		"stylelint": "^15.10.3",
		"stylelint-config-recess-order": "^4.3.0",
		"stylelint-config-sass-guidelines": "^10.0.0",
		"stylelint-config-standard-scss": "^11.0.0",
		"typescript": "~5.1.3"
	}
}
